    "clean:dist": "rimraf ./dist",
    "clean:target": "rimraf ./target",
    "generatepath": "generate-manifest-constants ./manifest.yaml",
    "install-urcap": "install-urcap 'target/sidebar-contribution*.urcapx'",
    "delete-urcap": "delete-urcap funh sidebar-contribution"
